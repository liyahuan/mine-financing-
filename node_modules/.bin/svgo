#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/repos/mangosteen-fe-1/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/svgo@2.8.0/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules:/root/repos/mangosteen-fe-1/node_modules/svgo/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/svgo/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/repos/mangosteen-fe-1/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/svgo@2.8.0/node_modules/svgo/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/svgo@2.8.0/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules:/root/repos/mangosteen-fe-1/node_modules/svgo/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/svgo/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svgo/bin/svgo" "$@"
else
  exec node  "$basedir/../svgo/bin/svgo" "$@"
fi
