#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/repos/mangosteen-fe-1/node_modules/.pnpm/vite@2.9.1/node_modules/vite/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vite@2.9.1/node_modules/vite/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vite@2.9.1/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@vitejs+plugin-vue@2.3.1_vite@2.9.1+vue@3.2.31/node_modules/@vitejs/plugin-vue/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@vitejs+plugin-vue@2.3.1_vite@2.9.1+vue@3.2.31/node_modules/@vitejs/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@vitejs+plugin-vue@2.3.1_vite@2.9.1+vue@3.2.31/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vite@2.9.1/node_modules/vite/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vite@2.9.1/node_modules/vite/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vite@2.9.1/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@vitejs+plugin-vue@2.3.1_vite@2.9.1+vue@3.2.31/node_modules/@vitejs/plugin-vue/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@vitejs+plugin-vue@2.3.1_vite@2.9.1+vue@3.2.31/node_modules/@vitejs/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@vitejs+plugin-vue@2.3.1_vite@2.9.1+vue@3.2.31/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@2.9.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@2.9.1/node_modules/vite/bin/vite.js" "$@"
fi
