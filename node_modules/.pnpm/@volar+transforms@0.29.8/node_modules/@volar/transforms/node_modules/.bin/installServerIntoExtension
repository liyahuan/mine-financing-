#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/repos/mangosteen-fe-1/node_modules/.pnpm/vscode-languageserver@8.0.0-next.10/node_modules/vscode-languageserver/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vscode-languageserver@8.0.0-next.10/node_modules/vscode-languageserver/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vscode-languageserver@8.0.0-next.10/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@volar+transforms@0.29.8/node_modules/@volar/transforms/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@volar+transforms@0.29.8/node_modules/@volar/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@volar+transforms@0.29.8/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vscode-languageserver@8.0.0-next.10/node_modules/vscode-languageserver/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vscode-languageserver@8.0.0-next.10/node_modules/vscode-languageserver/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vscode-languageserver@8.0.0-next.10/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@volar+transforms@0.29.8/node_modules/@volar/transforms/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@volar+transforms@0.29.8/node_modules/@volar/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/@volar+transforms@0.29.8/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vscode-languageserver@8.0.0-next.10/node_modules/vscode-languageserver/bin/installServerIntoExtension" "$@"
else
  exec node  "$basedir/../../../../../../vscode-languageserver@8.0.0-next.10/node_modules/vscode-languageserver/bin/installServerIntoExtension" "$@"
fi
