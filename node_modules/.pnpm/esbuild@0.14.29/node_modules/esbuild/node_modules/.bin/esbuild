#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/repos/mangosteen-fe-1/node_modules/.pnpm/esbuild@0.14.29/node_modules/esbuild/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/esbuild@0.14.29/node_modules/esbuild/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/esbuild@0.14.29/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/repos/mangosteen-fe-1/node_modules/.pnpm/esbuild@0.14.29/node_modules/esbuild/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/esbuild@0.14.29/node_modules/esbuild/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/esbuild@0.14.29/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
