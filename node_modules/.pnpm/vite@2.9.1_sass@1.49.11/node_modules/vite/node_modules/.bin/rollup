#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/repos/mangosteen-fe-1/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/dist/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/dist/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/rollup@2.70.1/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vite@2.9.1_sass@1.49.11/node_modules/vite/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vite@2.9.1_sass@1.49.11/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/repos/mangosteen-fe-1/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/dist/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/dist/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/rollup@2.70.1/node_modules/rollup/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/rollup@2.70.1/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vite@2.9.1_sass@1.49.11/node_modules/vite/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vite@2.9.1_sass@1.49.11/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@2.70.1/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../rollup@2.70.1/node_modules/rollup/dist/bin/rollup" "$@"
fi
