#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/root/repos/mangosteen-fe-1/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/typescript@4.6.3/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vue-tsc@0.29.8_typescript@4.6.3/node_modules/vue-tsc/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vue-tsc@0.29.8_typescript@4.6.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/root/repos/mangosteen-fe-1/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/typescript@4.6.3/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/node_modules:/root/repos/mangosteen-fe-1/node_modules:/root/repos/node_modules:/root/node_modules:/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vue-tsc@0.29.8_typescript@4.6.3/node_modules/vue-tsc/node_modules:/root/repos/mangosteen-fe-1/node_modules/.pnpm/vue-tsc@0.29.8_typescript@4.6.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.6.3/node_modules/typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.6.3/node_modules/typescript/bin/tsc" "$@"
fi
